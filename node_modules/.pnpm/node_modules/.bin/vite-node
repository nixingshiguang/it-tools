#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/it-tools/node_modules/.pnpm/vite-node@0.34.0_@types+node@18.15.11_less@4.1.3_terser@5.37.0/node_modules/vite-node/node_modules:/workspace/it-tools/node_modules/.pnpm/vite-node@0.34.0_@types+node@18.15.11_less@4.1.3_terser@5.37.0/node_modules:/workspace/it-tools/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/it-tools/node_modules/.pnpm/vite-node@0.34.0_@types+node@18.15.11_less@4.1.3_terser@5.37.0/node_modules/vite-node/node_modules:/workspace/it-tools/node_modules/.pnpm/vite-node@0.34.0_@types+node@18.15.11_less@4.1.3_terser@5.37.0/node_modules:/workspace/it-tools/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite-node/vite-node.mjs" "$@"
else
  exec node  "$basedir/../vite-node/vite-node.mjs" "$@"
fi
