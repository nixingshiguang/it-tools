#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/it-tools/node_modules/.pnpm/degit@2.8.4/node_modules/degit/node_modules:/workspace/it-tools/node_modules/.pnpm/degit@2.8.4/node_modules:/workspace/it-tools/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/it-tools/node_modules/.pnpm/degit@2.8.4/node_modules/degit/node_modules:/workspace/it-tools/node_modules/.pnpm/degit@2.8.4/node_modules:/workspace/it-tools/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../degit@2.8.4/node_modules/degit/degit" "$@"
else
  exec node  "$basedir/../../../../../degit@2.8.4/node_modules/degit/degit" "$@"
fi
