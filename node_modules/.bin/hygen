#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/it-tools/node_modules/.pnpm/hygen@6.2.11/node_modules/hygen/dist/node_modules:/workspace/it-tools/node_modules/.pnpm/hygen@6.2.11/node_modules/hygen/node_modules:/workspace/it-tools/node_modules/.pnpm/hygen@6.2.11/node_modules:/workspace/it-tools/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/it-tools/node_modules/.pnpm/hygen@6.2.11/node_modules/hygen/dist/node_modules:/workspace/it-tools/node_modules/.pnpm/hygen@6.2.11/node_modules/hygen/node_modules:/workspace/it-tools/node_modules/.pnpm/hygen@6.2.11/node_modules:/workspace/it-tools/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../hygen/dist/bin.js" "$@"
else
  exec node  "$basedir/../hygen/dist/bin.js" "$@"
fi
